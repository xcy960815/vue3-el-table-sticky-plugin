{
  "compilerOptions": {
    "isolatedModules": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "module": "esnext",
    "noImplicitAny": false,
    "removeComments": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "preserve",
    "noLib": false,
    "target": "es5",
    "lib": ["ESNext", "DOM"],
    "paths": {
      "@/*": ["plugin/*"]
    },
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationDir": "./temp"
  },
  "exclude": ["src", "plugin"],
  "include": [],
  "files": ["plugin/index.ts", "plugin/table-sticky.ts", "plugin/type.ts", "plugin/utils.ts"]
}
